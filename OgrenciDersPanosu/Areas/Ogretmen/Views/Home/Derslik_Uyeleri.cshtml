@model IEnumerable<OgrenciDersPanosu.Models.OgrenciModel>
@{
    ViewBag.Title = "Derslik_Uyeleri";
}


<link href="~/Content/OgrenciNotlariniGoruntule.css" rel="stylesheet" type="text/css" />
@{
    <div class="tabloDiv">
        <table class="table table-striped">
            @Html.AntiForgeryToken()
            <tr>
                <th>Üye No</th>
                <th>Üye Adı</th>
                <th>Üye Soyadı</th>
            </tr>
            @{
                
                if (Model.Count() != 0)
                {
                    foreach (var ogrenci in Model)
                    {
                        <tr>
                            <th>@ogrenci.OgrenciId</th>
                            <th>@ogrenci.Ad</th>
                            <th>@ogrenci.Soyad</th>
                        </tr>
                    }
                }
            }
        </table>
    </div>
}
